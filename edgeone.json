{
    "name" : "nanoconnect",
    "version" : "1.0.0",
    "description" : "NanoConnect",
    "build" : {
        "command" : "npm install && npm run build",
        "output" : "dist"
    },
    "functions" : {
        "directory" : "functions",
        "runtime" : "nodejs"
    },
    "caches" : [
        {
            "source" : "/images/*",
            "cacheTtl" : 86400
        }
    ]
}